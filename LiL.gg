local function AimToPosition(Position)
    local AimX = ((Position.X - Mouse.X) + 0) / AimbotSensitivity
    local AimY = ((Position.Y - Mouse.Y - _G.prediction) + 0) / AimbotSensitivity
    return AimX, AimY
end

